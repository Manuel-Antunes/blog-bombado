{
  "typescript": true,
  "commands": [
    "./commands",
    "@adonisjs/core/build/commands/index.js",
    "@adonisjs/repl/build/commands",
    "@adonisjs/lucid/build/commands",
    "adonis-sail/build/commands",
    "@eidellev/inertia-adonisjs/build/commands",
    "adonis-lucid-filter/build/commands",
    "@adonisjs/bouncer/build/commands",
    "@adonisjs/mail/build/commands",
    "@rocketseat/adonis-bull/build/commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts",
    "Helpers": "helpers"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    {
      "file": "./start/inertia",
      "environment": [
        "web"
      ]
    },
    "./start/bouncer",
    "./start/events"
  ],
  "providers": [
    "./providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/session",
    "@adonisjs/view",
    "@adonisjs/shield",
    "@adonisjs/lucid",
    "@adonisjs/auth",
    "@eidellev/inertia-adonisjs",
    "@91codes/adonis-vite",
    "@adonisjs/ally",
    "adonis-lucid-filter",
    "@softwarecitadel/adonisjs-girouette",
    "@adonisjs/bouncer",
    "@adonisjs/lucid-slugify",
    "@adonisjs/mail",
    "@rocketseat/adonis-bull",
    "@adonisjs/redis",
    "@eidellev/adonis-stardust",
    "@adonisjs/attachment-lite",
    "@adonisjs/drive-s3",
    "adonis-fcm"
  ],
  "metaFiles": [
    {
      "pattern": "public/**",
      "reloadServer": false
    },
    {
      "pattern": "resources/views/**/*.edge",
      "reloadServer": false
    }
  ],
  "aceProviders": [
    "@adonisjs/repl"
  ],
  "tests": {
    "suites": [
      {
        "name": "functional",
        "files": [
          "tests/functional/**/*.spec(.ts|.js)"
        ],
        "timeout": 60000
      }
    ]
  },
  "testProviders": [
    "@japa/preset-adonis/TestsProvider"
  ]
}
